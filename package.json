  "scripts": {
    "vscode:prepublish": "npm run package",
    "package": "npm run update-shortcuts && webpack --mode production --devtool hidden-source-map",
    "watch": "npm run compile && webpack --mode development --watch",
    "compile": "npm run update-shortcuts && webpack --mode development",
    "compile-tests": "tsc -p ./",
    "update-shortcuts": "esrun './src/Utils/UpdatePackageShortcuts.ts'",
    "watch-tests": "tsc -p ./ -w",
    "pretest": "npm run compile && npm run compile-tests",
    "test": "vscode-test",
    "lint": "eslint --ext ts src test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
